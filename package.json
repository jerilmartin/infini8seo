{
  "name": "content-factory-saas",
  "version": "1.0.0",
  "description": "Autonomous AI-powered content generation platform with two-phase research and production pipeline",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "test": "node test-system.js",
    "dev:server": "node server/index.js",
    "dev:worker": "node worker/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:worker\"",
    "start:server": "node server/index.js",
    "start:worker": "node worker/index.js",
    "cleanup": "node scripts/auto-cleanup.js",
    "build:client": "cd client && npm run build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "content-generation",
    "ai",
    "gemini",
    "saas",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.45.4",
    "axios": "^1.7.7",
    "bullmq": "^5.13.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "p-limit": "^6.1.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7"
  }
}

